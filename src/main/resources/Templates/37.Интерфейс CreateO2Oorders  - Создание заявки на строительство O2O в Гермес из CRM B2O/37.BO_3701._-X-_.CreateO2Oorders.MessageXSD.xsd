	<!--Система-получатель. Передается значение «HRMS»-->
	<xs:simpleType name="toCode">
		<xs:annotation>
			<xs:documentation>Система-получатель. Передается значение «HRMS»</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="HRMS">
