	<xs:complexType name="SubmitRequestOrder">
		<xs:all>
			<xs:element name="orderId" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<!-- Номер заказа в системе-источнике -->
			<xs:element name="orderOMSId" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<!-- Уникальный номер заказа сгенерированный в системе управления заказами -->
			<xs:element name="orderType" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<!-- Тип заказа согласно шагам бизнес-процессов управления услугами. Пример справочника см. tns:OrderType -->
			<xs:element name="orderParentId" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<!-- Ссылка на родительский заказ -->
			<xs:element name="orderPriority" type="xs:long" minOccurs="0" maxOccurs="1"/>
			<!-- Приоритет обработки заказа -->
			<xs:element name="orderChannel" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<!-- Канал продаж -->
			<xs:element name="branch" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<!-- Код макрорегионального филиала -->
			<xs:element name="affiliate" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<!-- Код регионального филиала -->
			<xs:element name="orderAttributes" type="tns:Attributes" minOccurs="0" maxOccurs="1"/>
			<!-- Атрибуты заказа -->
			<xs:element name="orderDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			<!-- Дата обращения клиента по коммерческому заказу -->
			<xs:element name="orderRequestedStartDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			<!-- Желаемая дата начала обработки заказа -->
			<xs:element name="orderRequiredDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			<!-- Желаемая дата исполнения заказа. Интерпретация параметра определяется бизнес-требованиями. -->
			<xs:element name="orderComments" type="tns:Comments" minOccurs="0" maxOccurs="1"/>
			<!-- Комментарии к заказу -->
			<xs:element name="orderLocation" type="tns:Location" minOccurs="0" maxOccurs="1"/>
			<!-- Адресная характеристика заказа -->
			<xs:element name="orderParties" type="tns:OrderParties" minOccurs="0" maxOccurs="1"/>
			<!-- Список объектов заказа -->
			<xs:element name="orderItems" type="tns:RequestOrderItems" minOccurs="1" maxOccurs="1"/>
			<!-- Список компонентов заказа -->
			<xs:element name="orderAppointmentId" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<!-- Идентификатор наряда WFM для визита к клиенту -->
		</xs:all>
	</xs:complexType>
