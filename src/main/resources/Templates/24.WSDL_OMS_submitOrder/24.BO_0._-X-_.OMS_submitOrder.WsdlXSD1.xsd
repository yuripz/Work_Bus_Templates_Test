	<xs:complexType name="SubmitOrderRequest">
		<xs:sequence>
			<xs:element name="originator" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<!-- Система - источник заказа -->
			<xs:element name="receiver" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<!-- Система - адресат заказа -->
			<xs:element name="callbackEndpoint" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<!-- callback URL для отправки нотификации по статусам и результата выполнения -->
			<xs:element name="mode" type="tns:OrderMode" minOccurs="0" maxOccurs="1"/>
			<!-- Режим обработки (синхронный, асинхронный) -->
			<xs:element name="requestId" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<!-- Идентификатор транзакции на вызывающей стороне -->
			<xs:element name="order" type="tns:SubmitRequestOrder" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubmitOrderResponse">
