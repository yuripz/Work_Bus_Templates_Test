			<xs:element name="orderComments" type="Comments" minOccurs="0" maxOccurs="1"/>
			<!-- Комментарии к заказу -->
			<xs:element name="orderLocation" type="Location" minOccurs="0" maxOccurs="1"/>
			<!-- Адресная характеристика заказа -->
			<xs:element name="orderParties" type="OrderParties" minOccurs="0" maxOccurs="1"/>
			<!-- Список объектов заказа -->
			<xs:element name="orderItems" type="RequestOrderItems" minOccurs="1" maxOccurs="1"/>
