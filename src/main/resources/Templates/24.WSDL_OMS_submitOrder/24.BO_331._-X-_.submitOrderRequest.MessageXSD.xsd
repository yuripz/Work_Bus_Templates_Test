		<xs:restriction base="xs:string">
			<xs:enumeration value="SUBMIT"/> 
			<xs:enumeration value="CANCEL"/> 
			<xs:enumeration value="QUERY"/> 
		</xs:restriction>
	</xs:simpleType> --> <!-- 1.10 Удалено -->
	<xs:simpleType name="OrderType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="CHECK"/> <!-- Проверка технической возможности -->
			<xs:enumeration value="PREBUILD"/> <!-- Проработка технической возможности -->
			<xs:enumeration value="ESTIMATE"/> <!-- Пересчет ТЭО по новым доходным параметрам -->
			<xs:enumeration value="INVEST"/> <!-- Запрос на выделение инвестиций -->
			<xs:enumeration value="BUILD"/> <!-- Организация технической возможности -->
			<xs:enumeration value="PROVIDE"/> <!-- Подключение -->
			<xs:enumeration value="ACTIVATE"/> <!-- Активация -->
			<xs:enumeration value="COMMIT"/> <!-- Завершение подключения -->
			</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SubmitRequestOrder">
		<xs:all>
			<xs:element minOccurs="1" maxOccurs="1" name="orderId" type="xs:string"/> <!-- Номер заказа в системе-источнике -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderType" type="OrderType"/> <!-- Тип заказа согласно шагам бизнес-процессов управления услугами -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderParentId" type="xs:string"/> <!-- Ссылка на родительский заказ -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderPriority" type="xs:long"/> <!-- Приоритет обработки заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderCategory" type="OrderCategory"/> <!-- Категория заказа по сегменту рынка -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderChannel" type="xs:string"/> <!-- Канал продаж -->
			<xs:element minOccurs="0" maxOccurs="1" name="branch" type="xs:string"/> <!-- Код макрорегионального филиала -->
			<xs:element minOccurs="0" maxOccurs="1" name="affiliate" type="xs:string"/> <!-- Код регионального филиала -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderAttributes" type="Attributes"/> <!-- Атрибуты заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderDate" type="xs:dateTime"/> <!-- Дата обращения клиента по коммерческому заказу -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderRequestedStartDate" type="xs:dateTime"/> <!-- Желаемая дата начала обработки заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderRequiredDate" type="xs:dateTime"/> <!-- Желаемая дата исполнения заказа. Интерпретация параметра определяется бизнес-требованиями. -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderComments" type="Comments"/> <!-- Комментарии к заказу -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderLocation" type="Location"/> <!-- Адресная характеристика заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderParties" type="OrderParties"/> <!-- Список объектов заказа -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderItems" type="RequestOrderItems"/> <!-- Список компонентов заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderAppointmentId" type="xs:string"/> <!-- Идентификатор наряда WFM для визита к клиенту -->
		</xs:all>
	</xs:complexType>
	<xs:complexType name="SubmitResponseOrder">
		<xs:choice>
			<xs:element minOccurs="1" maxOccurs="1" name="response" type="SyncSubmitResponseOrder"/>
			<xs:element minOccurs="1" maxOccurs="1" name="acknowledgement" type="AsyncSubmitResponseOrder"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="SyncSubmitResponseOrder">
		<xs:all>
			<xs:element minOccurs="1" maxOccurs="1" name="orderId" type="xs:string"/> <!-- Номер заказа в системе-источнике -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderOMSId" type="xs:string"/> <!-- Уникальный номер заказа сгенерированный в системе управления заказами -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderOMSurl" type="xs:string"/> <!-- ССылка на заказ в системе управления заказами -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderAttributes" type="Attributes"/> <!-- Атрибуты заказа -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderState" type="OrderState"/> <!-- Статус заказа -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderStartDate" type="xs:dateTime"/> <!-- Фактическая дата начала обработки заказа -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderCompletionDate" type="xs:dateTime"/> <!-- Фактическая дата завершения обработки заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItems" type="ResponseOrderItems"/> <!-- Список компонентов заказа --> <!-- 1.10 optional -->
		</xs:all>
	</xs:complexType>
	<xs:complexType name="AsyncSubmitResponseOrder">
		<xs:all>
			<xs:element minOccurs="1" maxOccurs="1" name="orderId" type="xs:string"/> <!-- Номер заказа в системе-источнике -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderOMSId" type="xs:string"/> <!-- Уникальный номер заказа сгенерированный в системе управления заказами -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderOMSurl" type="xs:string"/> <!-- ССылка на заказ в системе управления заказами -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderStartDate" type="xs:dateTime"/> <!-- Фактическая дата начала обработки заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderExpectedCompletionDate" type="xs:dateTime"/> <!-- Ожидаемая дата завершения обработки заказа -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderState" type="OrderState"/> <!-- 1.11 Статус заказа -->
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CancelRequestOrder">
		<xs:all>
			<xs:element minOccurs="0" maxOccurs="1" name="orderId" type="xs:string"/> <!-- Номер заказа в системе-источнике -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderOMSId" type="xs:string"/> <!-- Уникальный номер заказа сгенерированный в системе управления заказами -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderCategory" type="OrderCategory"/> <!-- Категория заказа по сегменту рынка -->
