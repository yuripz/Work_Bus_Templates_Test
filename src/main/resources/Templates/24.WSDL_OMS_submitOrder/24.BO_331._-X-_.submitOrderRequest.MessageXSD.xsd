			<xs:element minOccurs="0" maxOccurs="1" name="orderItemParentInstanceId" type="xs:string"/> <!-- Ссылка на orderItemInstanceId родительского компонента -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemPriority" type="xs:long"/> <!-- Приоритет обработки компонента в рамках выполнения заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemRequiredDate" type="xs:dateTime"/> <!-- Желаемая дата исполнения действия над компонентом. Интерпретация параметра определяется бизнес-требованиями. -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemSpecification" type="OrderItemSpecification"/> <!-- Спецификация компонента заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemLocation" type="Location"/> <!-- Адресная характеристика компонента заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemComments" type="Comments"/> <!-- Комментарии к компоненту заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemReservationId" type="xs:string"/> <!-- Идентификатор брони линейных данных в СЛТУ -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemAttributes" type="InheritableAttributes"/> <!-- Атрибуты компонента заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemBillingInfo" type="OrderItemBillingInfo"/> <!-- Биллинговая информация по компоненту заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderItemAppointmentId" type="xs:string"/> <!-- Идентификатор наряда WFM для визита к клиенту, в рамках которого выполняется инсталляция услуги -->
		</xs:all>
	</xs:complexType>
