<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" version="1.15">
	<xs:element name="submitOrderRequest" type="SubmitOrderRequest"/>
	<!-- Начать выполнение заказа -->
	<xs:complexType name="SubmitOrderRequest">
		<xs:sequence>
			<xs:element minOccurs="1" maxOccurs="1" name="originator" type="xs:string"/> <!-- Система - источник заказа -->
			<xs:element minOccurs="1" maxOccurs="1" name="receiver" type="xs:string"/> <!-- Система - адресат заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="callbackEndpoint" type="xs:string"/> <!-- callback URL для отправки нотификации по статусам и результата выполнения -->
			<xs:element minOccurs="0" maxOccurs="1" name="mode" type="OrderMode"/> <!-- Режим обработки (синхронный, асинхронный) -->
			<xs:element minOccurs="1" maxOccurs="1" name="requestId" type="xs:string"/> <!-- Идентификатор транзакции на вызывающей стороне -->
			<xs:element minOccurs="1" maxOccurs="1" name="order" type="SubmitRequestOrder"/>
		</xs:sequence>
	</xs:complexType>
	
