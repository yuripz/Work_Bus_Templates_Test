	<xs:simpleType name="OrderTaskState">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PLANNED"/> <!-- Задание создано, но выполнение задания не начато -->
			<xs:enumeration value="INPROGRESS"/> <!-- Задание выполняется -->
			<xs:enumeration value="PENDING"/> <!-- Задание в ожидании внешнего события -->
			<xs:enumeration value="COMPLETED"/> <!-- Задание выполнено -->
			<xs:enumeration value="CANCELLED"/> <!-- Задание отменено -->
			<xs:enumeration value="ONHOLD"/> <!-- Задание остановлено из-за ошибки и находится в ожидании решения проблемы -->
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ResponseOrderTask">
		<xs:all>
			<xs:element minOccurs="1" maxOccurs="1" name="orderTaskId" type="xs:string"/> <!-- Идентификатор задания -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderTaskName" type="xs:string"/> <!-- Название задания -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderTaskGroupName" type="xs:string"/> <!-- Название группы заданий -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderTaskStartDate" type="xs:dateTime"/> <!-- Фактическое время начала выполнения задания -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderTaskCompletionDate" type="xs:dateTime"/> <!-- Фактическое время завершения выполнения задания -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderTaskExpectedStartDate" type="xs:dateTime"/> <!-- Ожидаемая дата начала выполнения задания -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderTaskExpectedCompletionDate" type="xs:dateTime"/> <!-- Ожидаемая дата завершения выполнения задания -->
