	<xs:complexType name="CancelResponseOrder">
		<xs:all>
			<xs:element minOccurs="0" maxOccurs="1" name="orderId" type="xs:string"/> <!-- Номер заказа в системе-источнике -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderOMSId" type="xs:string"/> <!-- Уникальный номер заказа сгенерированный в системе управления заказами -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderState" type="OrderState"/> <!-- Статус заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderStartDate" type="xs:dateTime"/> <!-- Фактическая дата начала обработки заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderCompletionDate" type="xs:dateTime"/> <!-- Фактическая дата завершения обработки заказа -->
		</xs:all>
	</xs:complexType>
    <xs:simpleType name="AmendmentCode"><xs:annotation><xs:documentation>Тип изменения к исполняемому заказу</xs:documentation></xs:annotation>
        <xs:restriction base="xs:string">
			<xs:enumeration value="ORDERITEMS"><xs:annotation><xs:documentation>Изменения в составе и параметрах компонентов заказа</xs:documentation></xs:annotation></xs:enumeration>
			<xs:enumeration value="ORDERATTRIBUTES"><xs:annotation><xs:documentation>Изменения в параметрах заказа</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="GATEPASSED"><xs:annotation><xs:documentation>Внешний процесс прошел точку синхронизации</xs:documentation></xs:annotation></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
	<xs:complexType name="AmendRequestOrder">
		<xs:all>
			<xs:element minOccurs="0" maxOccurs="1" name="orderId" type="xs:string"/> <!-- Номер заказа в системе-источнике -->
			<xs:element minOccurs="1" maxOccurs="1" name="orderOMSId" type="xs:string"/> <!-- Уникальный номер заказа сгенерированный в системе управления заказами -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderCategory" type="OrderCategory"/> <!-- Категория заказа по сегменту рынка -->
			<xs:element minOccurs="0" maxOccurs="1" name="branch" type="xs:string"/> <!-- Код макрорегионального филиала -->
			<xs:element minOccurs="0" maxOccurs="1" name="affiliate" type="xs:string"/> <!-- Код регионального филиала -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderAttributes" type="Attributes"/> <!-- Атрибуты заказа -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderComments" type="Comments"/> <!-- Комментарии к заказу -->
			<xs:element minOccurs="0" maxOccurs="1" name="orderParties" type="OrderParties"/> <!-- Список объектов заказа -->
